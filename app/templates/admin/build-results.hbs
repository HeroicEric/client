<table>
  <thead>
    <tr>
      <th>Addon</th>
      <th>Version</th>
      <th>Date/time</th>
      <th>Result</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
  {{#each sortedBuildResults as |testResult|}}
    <tr class="test-build-result" data-testResultId={{testResult.id}}>
      <td>{{testResult.version.addon.name}}</td>
      <td>{{testResult.version.version}}</td>
      <td>{{moment-format testResult.testsRunAt 'YYYY-MM-DD HH:mm'}}</td>
      <td></td>
      <td></td>
    </tr>
  {{/each}}
  </tbody>
</table>
